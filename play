<script defer="" type="text/javascript" src="https://www.rareanimes.co/wp-content/cache/flying-press/91954b488a9b.akismet-frontend.js" id="akismet-frontend-js"></script>
<script type="text/javascript">
const dayMap = {
  "sunday": 0, "monday": 1, "tuesday": 2, "wednesday": 3, 
  "thursday": 4, "friday": 5, "saturday": 6
};

const episodeTextElem = document.getElementById("episodeText");
if (episodeTextElem) {
  let originalText = episodeTextElem.textContent;
  const curlyRegex = /{(Sunday|Monday|Tuesday|Wednesday|Thursday|Friday|Saturday)-(\d{1,2}):(\d{2})}/i;
  const match = originalText.match(curlyRegex);

  if (match) {
    let targetDayStr = match[1];
    let targetDayNum = dayMap[targetDayStr.toLowerCase()];
    let targetHour = parseInt(match[2], 10);
    let targetMinute = parseInt(match[3], 10);

    function updateCountdown() {
      const now = new Date();
      let currentDayNum = now.getDay();

      let diffDays = (targetDayNum - currentDayNum + 7) % 7;

      let nextOccurrence = new Date(now);
      nextOccurrence.setDate(now.getDate() + diffDays);
      nextOccurrence.setHours(targetHour, targetMinute, 0, 0);

      if (diffDays === 0 && now >= nextOccurrence) {
        // If it's today but already passed, move to next week
        nextOccurrence.setDate(nextOccurrence.getDate() + 7);
      }

      let diffInSeconds = Math.floor((nextOccurrence - now) / 1000);

      if (diffInSeconds <= 0) {
        // Event has started, replace with formatted time
        let hourDisplay = (targetHour % 12) || 12;
        let period = targetHour < 12 ? "AM" : "PM";
        let replacement = `${targetDayStr} at ${hourDisplay}:${String(targetMinute).padStart(2, '0')} ${period}`;
        episodeTextElem.textContent = originalText.replace(curlyRegex, replacement);
        return;
      }

      let countdownStr = "";
      if (diffInSeconds >= 86400) {
        let days = Math.floor(diffInSeconds / 86400);
        let hours = Math.floor((diffInSeconds % 86400) / 3600);
        countdownStr = `${days} day${days === 1 ? "" : "s"}${hours > 0 ? ` ${hours} hour${hours === 1 ? "" : "s"}` : ""}`;
      } else if (diffInSeconds >= 3600) {
        let hours = Math.floor(diffInSeconds / 3600);
        let minutes = Math.floor((diffInSeconds % 3600) / 60);
        countdownStr = `${hours} hour${hours === 1 ? "" : "s"}${minutes > 0 ? ` ${minutes} minute${minutes === 1 ? "" : "s"}` : ""}`;
      } else {
        let minutes = Math.floor(diffInSeconds / 60);
        let seconds = diffInSeconds % 60;
        countdownStr = `${minutes > 0 ? `${minutes} minute${minutes === 1 ? "" : "s"}` : ""} ${seconds > 0 ? `${seconds} second${seconds === 1 ? "" : "s"}` : ""}`.trim();
      }

      episodeTextElem.textContent = originalText.replace(curlyRegex, countdownStr);
    }

    updateCountdown();
    setInterval(updateCountdown, 1000);
  }
}

</script>

